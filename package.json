    "@google/generative-ai": "^0.1.3",
    "@pinecone-database/pinecone": "^6.1.2",
    "@xenova/transformers": "^2.17.2",
    "adm-zip": "^0.5.16",
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.2.1",
