{
  "name": "designer-bff",
  "version": "1.0.0",
  "description": "AI-powered design platform with voice commands, automated image generation, and intelligent feedback loops",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "debug": "node debug-server.js",
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "test": "jest",
    "test:watch": "jest --watch",
    "migrate": "knex migrate:latest",
    "seed": "knex seed:run",
    "scheduler": "node src/jobs/nightly-generator.js"
